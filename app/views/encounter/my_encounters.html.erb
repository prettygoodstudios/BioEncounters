<div class="content-card">
  <h1><%= current_user.display %>'s Encounters</h1>
  <%= render partial: "encounter/table", locals: {encounters: @encounters} if @encounters.length != 0%>
  <%= "<h3>You currently have no encounters.</h3>".html_safe if @encounters.length == 0 %>
  <br />
  <%= back_button({ path: root_path}) %>
</div>
