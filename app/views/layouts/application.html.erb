<!DOCTYPE html>
<html>
  <head>
    <title>Bioencounters</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <script>
      const ROOT_URL = "<%= root_url %>";
      console.log(ROOT_URL);
    </script>
    <%= javascript_include_tag 'application' %>
    <%= javascript_include_tag 'alert' %>
  </head>
  <body>
    <div class="push">
      <%= render partial: "sharred/nav" %>
      <div class="container-wrapper">
        <div class="container">
          <%= render partial: "sharred/alert", locals: {alert: alert, notice: notice} %>
          <%= yield %>
        </div>
      </div>
    </div>
    <%= render partial: "sharred/footer" %>
    <%= javascript_include_tag 'nav' %>
  </body>
</html>
